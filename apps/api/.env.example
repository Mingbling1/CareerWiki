# API Environment
PORT=4000
NODE_ENV=development

# Database
DATABASE_URL=postgresql://empliq:empliq_dev_password@postgres:5432/empliq

# CORS
CORS_ORIGINS=http://localhost:5173,http://localhost:3000

# Supabase (for auth verification)
SUPABASE_URL=your-supabase-url
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Oracle Object Storage
# PAR URL for uploading (with write permission)
ORACLE_PAR_UPLOAD_URL=https://objectstorage.sa-saopaulo-1.oraclecloud.com/p/YOUR_PAR_TOKEN/n/YOUR_NAMESPACE/b/YOUR_BUCKET/o/
# Public base URL for reading uploaded files
ORACLE_PUBLIC_URL_BASE=https://objectstorage.sa-saopaulo-1.oraclecloud.com/n/YOUR_NAMESPACE/b/YOUR_BUCKET/o/
