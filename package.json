{
  "name": "empliq",
  "version": "0.1.0",
  "private": true,
  "description": "Plataforma de transparencia laboral - Empliq",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=apps/frontend",
    "dev:website": "npm run dev --workspace=apps/website",
    "build": "npm run build --workspace=apps/frontend",
    "build:website": "npm run build --workspace=apps/website",
    "docker:dev": "docker-compose -f docker/docker-compose.dev.yml up",
    "docker:build": "docker-compose -f docker/docker-compose.dev.yml build"
  },
  "keywords": [
    "empliq",
    "salarios",
    "transparencia",
    "laboral",
    "organigrama",
    "react-flow"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tu-usuario/empliq"
  }
}
